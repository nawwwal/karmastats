{
  "version": 2,
  "functions": {
    "api/ssr.js": {
      "includeFiles": "dist/**"
    }
  },
  "rewrites": [
    {
      "source": "/assets/(.*)",
      "destination": "/assets/$1"
    },
    {
      "source": "/(favicon\\.svg|og-default\\.png|robots\\.txt|sitemap\\.xml|llms\\.txt|qr-code-coffee\\.jpeg|qr-code-tea\\.jpeg)",
      "destination": "/$1"
    },
    {
      "source": "/(.*\\.(?:css|js|map|json|png|jpg|jpeg|webp|gif|svg|ico|txt|xml))",
      "destination": "/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/api/ssr"
    }
  ]
}